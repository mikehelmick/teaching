		JFrame frame = new JFrame("Pong");
		frame.setDefaultCloseOperation( JFrame.EXIT_ON_CLOSE );
		frame.setPreferredSize( new Dimension(WIDTH,HEIGHT) );
		frame.setResizable( false );
		
